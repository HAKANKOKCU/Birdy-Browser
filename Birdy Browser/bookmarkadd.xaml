<Window x:Class="Birdy_Browser.bookmarkadd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Birdy_Browser"
        mc:Ignorable="d"
        Title="Add Bookmark" Width="554" WindowStartupLocation="CenterOwner" SizeToContent="Height">
    <StackPanel>
        <DockPanel>
            <Label>Name</Label>
            <TextBox Name="nameTB"></TextBox>
        </DockPanel>
        <DockPanel>
            <Label>Icon Path</Label>
            <TextBox Name="iconTB"></TextBox>
        </DockPanel>
        <DockPanel>
            <Label>URL</Label>
            <TextBox Name="urlTB"></TextBox>
        </DockPanel>
        <Button Name="conf">Add</Button>
    </StackPanel>
</Window>
